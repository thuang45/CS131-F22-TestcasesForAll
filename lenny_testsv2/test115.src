func main void
  funccall input "Enter a number: "
  var string str_val
  assign str_val results
  funccall strtoint str_val
  var int n f
  assign n resulti
  funccall factorial n f
  funccall print resulti
endfunc

func factorial n:int f:int int
  assign f 1
  while > n 1
    assign f * f n
    assign n - n 1
  endwhile
  return f
endfunc
